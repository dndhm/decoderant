{
  "name": "decoderant",
  "version": "0.1.0",
  "keywords": [],
  "author": "Dan Needham <dneedham@gmail.com> (http://dndhm.com)",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.0",
    "eslint": "4.10.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.4.0",
    "express": "^4.16.2",
    "nodemon": "^1.17.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.1.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-jss": "^8.1.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "jest": "20.0.4",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "build": "webpack --config config/webpack.config.server.js && webpack --config config/webpack.config.client.js",
    "start": "nodemon build/server/index.js",
    "dev:hot": "webpack --config config/webpack.config.dev.js && node dev-server.js",
    "dev:server": "webpack --config config/webpack.config.server.js --watch",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:hot\" \"npm start\"",
    "test": "jest",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.spec.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ],
    "plugins": [
      "babel-plugin-transform-object-rest-spread"
    ]
  }
}
